<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_dashboard"
    app:startDestination="@id/productListFragment">

    <fragment
        android:id="@+id/productListFragment"
        android:name="com.example.android.ui.view.fragments.ProductListFragment"
        android:label="Product List"
        tools:layout="@layout/fragment_product_list" />

    <fragment
        android:id="@+id/userDetailFragment"
        android:name="com.example.android.ui.view.fragments.UserDetailFragment"
        android:label="Product Detail"
        tools:layout="@layout/fragment_user_detail" />

    <fragment
        android:id="@+id/notificationFragment"
        android:name="com.example.android.ui.view.fragments.NotificationFragment"
        android:label="Notification"
        tools:layout="@layout/fragment_notification" />

</navigation>